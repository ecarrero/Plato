{
	"welcomeFile": "/",
	"authenticationMethod": "none",
	"routes": [
		{
			"source": "^/sap/bc/ui5_ui5",
			"destination": "Plato"
		},
		{
			"source": "^/sap/bc/ui2/app_index",
			"destination": "Plato"
		},
		{
			"source": "^/sap/opu/odata",
			"destination": "Plato"
		},
		{
			"source": "^/sap/bc/lrep",
			"destination": "Plato"
		},
		{
			"source": "^/resources",
			"destination": "sapui5"
		},
		{
			"source": "^/test-resources",
			"destination": "sapui5"
		},
		{
			"source": "^/webapp/(resources/.*)$",
			"destination": "sapui5",
			"target": "$1"
		},
		{
			"source": "^/webapp/(test-resources/.*)$",
			"destination": "sapui5",
			"target": "$1"
		},
		{
			"source": "^(.*)$",
			"target": "$1",
			"localDir": "./"
		}
	]
}